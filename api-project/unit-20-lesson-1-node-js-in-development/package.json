{
  "name": "api-project",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint src/ --cache --fix",
    "test": "jest",
    "build": "tsc",
    "build:dev": "tsc --watch",
    "start": "node --enable-source-maps dist/server.js",
    "start:dev": "nodemon --watch dist/ --watch .env",
    "dev": "npm run build && concurrently 'npm:build:dev' 'npm:start:dev'",
    "web": "serve -p 8080 web/",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=16"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/simonteaching/api-project.git"
  },
  "bugs": {
    "url": "https://github.com/simonteaching/api-project/issues"
  },
  "homepage": "https://github.com/simonteaching/api-project#readme",
  "devDependencies": {
    "@tsconfig/node16": "^1.0.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/jest": "^27.5.1",
    "@types/mime": "^2.0.3",
    "@types/node": "^17.0.31",
    "@types/passport": "^1.0.9",
    "@types/passport-github2": "^1.2.5",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "concurrently": "^7.1.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "jest": "^28.0.3",
    "jest-mock-extended": "^2.0.6",
    "lint-staged": "^13.0.2",
    "nodemon": "^2.0.16",
    "prettier": "2.6.2",
    "prisma": "^3.14.0",
    "serve": "^13.0.2",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.2",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "@prisma/client": "^3.14.0",
    "@sinclair/typebox": "^0.23.5",
    "@types/multer": "^1.4.7",
    "ajv-formats": "^2.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "express-json-validator-middleware": "^3.0.0",
    "express-session": "^1.17.3",
    "mime": "^3.0.0",
    "multer": "^1.4.4",
    "passport": "^0.6.0",
    "passport-github2": "^0.1.12"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix",
    "*.{js,ts,md,html,css}": "prettier --write"
  }
}
